#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);
    long int t;
    cin>>t;
    while(t--)
    {
        long long int n,k,i=0;
        cin>>n>>k;
        long long int num,h[200005]={0};
        for(i=0;i<n;i++)
        {
            cin>>num;
            h[num]++;
        }
        i=0;
        while(k>=0)
        {
            if(!h[i])
                k--;
                i++;
        }
        cout<<i-1<<"\n";
    }
}
